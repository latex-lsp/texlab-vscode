{
  "comments": {
    "lineComment": "%"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["$", "$"],
    ["\\[", "\\]"],
    ["\\(", "\\)"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["`", "'"]
    {
      "open": "$",
      "close": "$",
      "notIn": ["comment"]
    },
    {
      "open": "\\[",
      "close": "\\]",
      "notIn": ["comment"]
    },
    {
      "open": "\\(",
      "close": "\\)",
      "notIn": ["comment"]
    }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["$", "$"],
    ["`", "'"]
  ]
}
